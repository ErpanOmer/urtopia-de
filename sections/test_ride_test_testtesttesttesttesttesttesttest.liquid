                      {% for value in values %}
                        <span>{{ value }}</span>
                      {% endfor %}
                    </div>
                  {% when 'Style' %}
                    <div attribute="{{ value }}" class="{{ value }}"><span class="tooltip">{{ value }}</span></div>
                  {% else %}
                    <div attribute="{{ value }}" class="{{ value }}">{{ value }}</div>
                {% endcase %}
              {% endfor %}
            </div>
          </div>
