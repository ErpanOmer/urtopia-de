{{ 'fusion-new.scss.css' | asset_url | stylesheet_tag: preload: true }}

{% assign product = all_products["urtopia-fusion-carbon-ebike"] %}

<h1 class="visuallyhidden">Urtopia Fusion</h1>
<div class="product" data-pf-type="ProductBox">
  <div class="centeral-content">
      <div class="commonComponent pcHide">
        <div class="u20DemiBold_v2">{{ product.title }}</div>
        <div data-pf-type="JudgeMe" class="sc-eZhRLC gtfCKx pf-25_ order-djs mb16">
          <div class="pf-judme">
            {% render 'judgeme_widgets', 
              widget_type: 'judgeme_preview_badge',
              jm_style: '',
              concierge_install: false,
              product: product
            %}
          </div>
        </div>
        <div
          class="product-price u24DemiBold_v2 mb12">
          {{ product.price |  money_without_trailing_zeros  }}
          <span class="u17Medium_v2" style="font-size: 15px;">{{ product.compare_at_price | money }}</span>
          <span class="u14Medium_v2" style="font-size: 13px;">-{{ product.compare_at_price |  minus: product.price |  money_without_trailing_zeros }}</span>
        </div>
        {% render "sale" %}
      </div>
    <div class="left-swiper mobileHide">
      <div class="swiper-wrapper">
        <img class="swiper-slide" alt="Urtopia Carbon 1 Pro" src="{{prelo}}"/>
      </div>
    </div>
    <span></span>
    <div class="featured_image mb16" style="--swiper-navigation-color: #000;--swiper-theme-color: #000;--swiper-pagination-color: #000;">
      <div class="right-swiper">
        <div class="swiper-wrapper">
          <div class="swiper-slide"><img alt="Urtopia Carbon 1 Pro" src="{{prelo}}"/></div>
        </div>
        <div class="arrow swiper-button-prev mobileHide"></div>
        <div class="arrow swiper-button-next mobileHide"></div>
      </div>
      <div class="medias">
         <i class="u17DemiBold_v2">Carbon Fiber Expert</i>
         <img src="https://newurtopia.com/cdn/shop/files/logo_0707__1.png?v=1684310857" alt="">
      </div>
      <div class="swiper-pagination pcHide"></div>
      <div class="free mobileHide">
        <span class="u14Medium_v2"><span style="color:#fd4b17;">Limited offer:</span> Fenders, kickstand, 1-year eSIM service</span>
        <span class="flex-1"></span>
        <del class="u14Light_v2">{{ 16700 | money }}</del>
        <span class="u17Medium_v2">Free</span>
      </div>
      <div class="icons mobileHide">
