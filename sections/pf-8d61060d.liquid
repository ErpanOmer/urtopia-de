<style>  
  
:root {
          --sale-color1: #FD4B17;
          --sale-color2: #FD4B17;
          --sale-color3: #FD4B17;
          --sale-color4: #FD4B17;
        }
.product .centeral-content .product-panel .options .Model .active {
    border-color: var(--sale-color3);
  }

  .product .centeral-content .product-panel .options .Model>div:after {
    background-color: var(--sale-color3);
  }

  .product .centeral-content [data-price]::after {
      content: attr(data-price);
      font-size: 24px;
      opacity: 0.75;
      text-decoration: line-through;
      margin-left: 5px;
  }

  .product .centeral-content .product-panel .options .Size>div:after,
  .product .centeral-content .product-panel .options .Model>div:after {
    display: none;
  }

  .product .centeral-content .product-panel .options .Model .active,
  .product .centeral-content .product-panel .options .Size .active {
    border-color: #000;
  }

  .product .centeral-content .product-panel .options .Size .active {
    padding: 9px 12px;
  }

  .product .list-payment__item {
    padding: 0.3rem;
  }

  .product .list-payment {
    margin: 0;
    padding: 0;
    justify-content: center;
  }

  .product .emphasize-list {
    box-shadow: #0000001a 0 0 6px;
    border-radius: 10px;
    padding: 10px 15px;
    margin: 0 0 16px;
    line-height: 1.8em;
  }

  .product .emphasize-list li {
      list-style: none;
      display: flex;
  }

  .product .emphasize-list li > span:first-child {
      color: rgb(253, 75, 23);
      margin-right: 5px;
      transform: translateY(-1px);
      display: inline-block;
  }

  .__pf_lUQL6LKn .pf-685_,
  .__pf_lUQL6LKn .pf-727_,
  .__pf_lUQL6LKn .pf-798_ {
    margin-top: 24px;
  }

  .__pf_lUQL6LKn .pf-808_ .u11Light_v2 {
      font-size: 12px;
      color: #999;
      line-height: 1.5em;
      margin-top: 24px;
  }

  .carry {
    margin-top: 100px;
  }


  .choose {
    font: 300 16px/1.4em urbane;
    text-decoration: underline;
    color: #EB5A2D;
    cursor: pointer;
  }

  .product .centeral-content .product-panel .options div[option="Size"] .u17DemiBold_v2 {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
  }

  .product .centeral-content .product-panel .options div[option="Size"] .u17DemiBold_v2 .u17Light_v2 {
    margin-right: 12px;
  }

  @media (max-width: 767px) {
      .__pf_lUQL6LKn .pf-685_,
    .__pf_lUQL6LKn .pf-727_,
    .__pf_lUQL6LKn .pf-798_ {
      margin-top: 0;
    }

    .__pf_lUQL6LKn .pf-808_ .u11Light_v2 {
        margin-top: 0;
    }

    .carry {
      margin-top: 60px;
    }

    .product .centeral-content .product-panel .options .Model>div {
      padding: 10px 12px;
    }

    .product .emphasize-list {
      font-size: 14px;
      margin-top: 6px;
    }
  }



  .product .centeral-content [data-price]::after {
    color: #000;
  }
  
</style>

