<script>
    // 默认隐藏
    $('.bottom-fixed').hide()
    $('.top-fixed').hide()
    $('.toTop').hide()
    
    const leftSwiper = new Swiper(".product .left-swiper", {
        direction: 'vertical',
        spaceBetween: 8,
        slidesPerView: 8,
        freeMode: true,
        watchSlidesProgress: true,
      })

      const rightSwiper = new Swiper(".product .right-swiper", {
        autoHeight: true,
        navigation: {
          nextEl: ".product .right-swiper .swiper-button-next",
          prevEl: ".product .right-swiper .swiper-button-prev",
        },
        pagination: {el:'.swiper-pagination'},
        effect: 'fade',
        fadeEffect:{
            crossFade: true
        },
        thumbs: {
          swiper: leftSwiper,
        },
      });

    document.addEventListener('onChangeModel', e => {
      $(`.product .product-panel .options > div[option="Model"] .u17DemiBold_v2 .u17Light_v2`).text(`Urtopia Carbon ${e.value === 'Carbon Belt' ? '1' : '1s'}`)
    })

    document.addEventListener('onChangeStyle', e => {
      $(`.product .product-panel .options > div[option="Style"] .u17DemiBold_v2 .u17Light_v2`).text(e.value)
    })

    document.addEventListener('onChangeSize', e => {
      $(`.product .product-panel .options > div[option="Size"] .u17DemiBold_v2 .u17Light_v2`).text(global_config.carbon_order_page_config.size_map[e.value])
    })

    document.addEventListener('onChangeOptions', e => {
            let swiper_arr = []
            swiper_arr = swiper_arr.concat(global_config.carbon_order_page_config.product_images[e.value.Model][e.value.Style], e.value.Style === 'Vanilla' ? [] : global_config.carbon_order_page_config.product_images[e.value.Model]['commonSwiper'])

            $('.product .right-swiper .swiper-wrapper').html(swiper_arr.map((s, i) => {
                const index = i % 6

                return  `<div class="swiper-slide">
                            <img src="${s}&width=700"/>
                            <div>
                                <span class="u20DemiBold_v2">${global_config.carbon_order_page_config.product_media_images[index].text}</span>
                                <img src="${global_config.carbon_order_page_config.product_media_images[index].img}&width=350">
                            </div>
                        </div>`
              }).join(''))
            $('.product .left-swiper .swiper-wrapper').html(swiper_arr.map(s => `<div class="swiper-slide"><img src="${s}&width=700"></div>`).join(''))
            
            // update
            setTimeout(() => {
                rightSwiper.update()
                rightSwiper.slideTo(0, 0)
                
                leftSwiper.update()
                leftSwiper.slideTo(0, 0)
            }, 0)

            $('.product .delivery_time').html(global_config.carbon_order_page_config.ebike_delivery_time[e.value.id] || global_config.carbon_order_page_config.ebike_default_delivery_time)

            const btn =  $('.product .buttons .addToCartBtn')
            if (e.value.available) {
                btn.find('span').text('Add to Cart')
                btn.removeClass('disabled')
                $('.product .buttons .orderTestRideBtn').show()
                $('.product .buttons .my-button-origin[onclick="buynow(this)"]').show()
            } else {
                btn.find('span').text('Sold out')
                btn.addClass('disabled')
                $('.product .buttons .orderTestRideBtn').hide()
                $('.product .buttons .my-button-origin[onclick="buynow(this)"]').hide()
            }

            /*
            const getContent = s => {
                options[1] = s

                const f = variants.find(i => i.title === optionsToTtitle(options))

                if (!f || !f.available) {
                  return '{{ 'order_page.out' | t }}'
                }

                return `{{ 'order_page.only' | t }} ${Math.max(f.inventory_quantity, 2)} {{ 'order_page.stock' | t }}`
            }
          
            setTimeout(() => {
              $(`.product .product-panel .Size > div[attribute="M"]`).attr('data-inventory-quantity', getContent('M'));
              $(`.product .product-panel .Size > div[attribute="L"]`).attr('data-inventory-quantity', getContent('L'));
            }, 0)
            */
    })


    function changeAddToCartText (parse, step = 0) {
        if (step) {
            $(".addToCartBtn i").toggleClass("showi");
            document.querySelector(".addToCartBtn span").innerHTML = "Adding...";
        } else {
            setTimeout(()=>{
                $(".addToCartBtn i").toggleClass("showi");
                document.querySelector(".addToCartBtn span").innerHTML = "Add to Cart";
            }, 800)
        }
    }

    function buynow (e) {

        const t = $(e)

        if (t.hasClass('loading')) {
            return
        }

        console.log(t)

        t.addClass('loading')
        t.html('<i class="fa fa-spinner fa-spin" style="margin-right: 8px;"></i>Processing...')

        const body = { items: [{ id: current_variant_id, quantity: 1 }] }
        $('.product .accessories .items .active').each((i, item) => {
          body.items.push({ 
            id: $(item).attr('variant-id'), 
            quantity: 1
          })
        })

        fetch(window.Shopify.routes.root + 'cart/add.js', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(body)
        })
        .then(response => {
            fetchBuried('websiteclick', `outlet-carbon-order-page`, { button: 'buynow' }, true)
            window.location.href="/checkout"
        })
        .catch((error) => {
          t.removeClass('loading')
          t.html('Buy now')

          console.error('Error:', error);
        })
}

    function testRideTrace(type = '', target){

        // 如果已经有按钮 loading ,直接退出来
        if (!type || $('.orderTestRideBtn').hasClass('loading')) {
            return
        }
    
        const t = $(target)
    
        t.addClass('loading')
        // 原本的html 存起来
        const text = t.find('span').html()
        const loading = '<i class="fa fa-spinner fa-spin"></i>Processing...'
    
        // loading
        t.find('span').html(loading)
    
        
    
        if (type === 'lease_on_jobrad') {
            fetchBuried('websiteclick', `outlet-carbon-order-page`, { button: type }, true)
    
            t.removeClass('loading')
            t.find('span').html(text)

            const url = window.sessionStorage.localPathPerfix + `/pages/ebike-leasing?bike_model=${current_variant.option1}&bike_style=${current_variant.option3}&bike_size=${current_variant.option2}&utm_source=order_page#forms`

            return window.open(url)
        }
    
        if (type === 'klarna_check_out') {
            fetchBuried('websiteclick', `outlet-carbon-order-page`, { button: type, action_type: sessionStorage.carbon_one_a_b_test_flag }, true)
            window.sessionStorage.clicked_klarna_checkout_button = 'clicked_klarna_checkout_button'
    
            setTimeout(() => {
                    t.removeClass('loading')
                    t.find('span').html(text)
                }, 2000)

            // 如果 B test 需要显示 klarna checkout
            window.show_notification_checkout_button = true
            return addToCartInsurance("atc", true)
        }
    
        if (type === 'shop_on_mediamarkt') {
            t.removeClass('loading')
            t.find('span').html(text)
            return window.open('https://www.mediamarkt.de/de/search.html?query=urtopia&filter=carType%3ACitybike')
          
        }
    }



    $('.product .product-panel .accessories .navigate > div').on('click', e => {
        const items = $('.product .product-panel .accessories .items')

        items.animate({
            scrollLeft: $('.product .product-panel .accessories .items').scrollLeft() + ($(e.target).hasClass('next') ? 180 : -180)
        }, 300)
    })

    $('.product .product-panel .accessories .items .item').on('click', e => {
        const item = $(e.target).closest('.item')
        const icon = item.find('.u11Light_v2')

        if (item.hasClass('active')) {
            item.removeClass('active')
            icon.html('<i class="fa-solid fa-plus"></i>')
            
        } else {
            item.addClass('active')
            icon.html('<i class="fa-solid fa-check"></i>')
        }
    })

    $('.product .product-panel').on("submit", function (event) {

        // 阻止默认行为
        event.preventDefault();
        return false;
    })

    $('.product .dialogs > div').on('click', function(e){
        const m = {
            0: 'one',
            1: 'two',
            2: 'three',
            3: 'four'
        }

      if ($(this).attr('onclick')) {
          return
        }

        $('.product .dialogs > div').each((i, t) => {
            if ($(e.target).closest('.u17Medium_v2')[0] === t) {
                console.log(i)

                const layer = document.querySelector(`.order-modal-${m[i]}`);
                const laybox = document.querySelector(`.order-modal-layer-${m[i]}`);
                layer.style.display = "flex";
                laybox.className += " dialog-open";
            }
        })
    })

    $('.toTop').on('click', e => {
        $('html').animate({
            scrollTop: 0
        }, 300)
    })


    let currnetScroolTop = 0
    $(document).on('scroll', throttle(e => {
        const top1 = $('#freedom').offset().top
        const top3 = $('.product .addToCartBtn').offset().top
        const top2 = $(e.target).scrollTop()

        const h = window.screen.height

        if (global_config.is_mobile) {
            if (top2 > top3) {
                $('.bottom-fixed').show()
            } else {
                $('.bottom-fixed').hide()
            }
        } else {
            if (top2 > top1) {
                $('.top-fixed').show()
            } else {
                $('.top-fixed').hide()
            }

            if (currnetScroolTop > top2) {
                $('.top-fixed').hide()
            }

            currnetScroolTop = top2
        }

        top2 > (h * 2) ? $('.toTop').show() : $('.toTop').hide()

    }, 300, 100))
</script>
