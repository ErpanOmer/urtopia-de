<script>
  let slideNo = 0;
  var swiperSectionWorld = new Swiper("#world-swiper", {
    autoplay: {
      delay: 0,
      disableOnInteraction: false
    },
    noSwiping: true,
    freeMode: true,
    loop: true,
    speed: 6000,
    slidesPerView: "auto"
  });
  let nextTransForm = "";

// 轮播图从暂停位置移动到原本应到的位置所用的时间
  let nextTime = 0;
  let slideWidth = 165;
  $("#world-swiper").hover(() => {
    slideWidth = document.querySelector("#world-swiper .swiper-slide.slide").offsetWidth;
    nextTransForm = document.querySelector("#world-swiper .swiper-wrapper").style.transform;

// 轮播图原本应移动到的位置
    let nextTransPosition = -1 * parseInt(document
      .querySelector("#world-swiper .swiper-wrapper")
      .style
      .transform
      .split("translate3d(")[1]
      .split("px")[0]);

// 鼠标悬浮时时轮播图位置
    let nowTransPosition = -1 * parseInt(window.getComputedStyle(document.querySelector("#world-swiper .swiper-wrapper"), false)["transform"].split("1, ")[2].split(",")[0]);
    let nowTransForm = window.getComputedStyle(document.querySelector("#world-swiper .swiper-wrapper"), false)["transform"];
    nextTime = 6000 * ((nextTransPosition - nowTransPosition) / slideWidth);
    document.querySelector("#world-swiper .swiper-wrapper").style.transform = nowTransForm;
    document.querySelector("#world-swiper .swiper-wrapper").style.transitionDuration = "0ms";
    swiperSectionWorld.autoplay.stop();
  }, () => {
    document.querySelector("#world-swiper .swiper-wrapper").style.transform = nextTransForm;
    document.querySelector("#world-swiper .swiper-wrapper").style.transitionDuration = nextTime + "ms";
    swiperSectionWorld.autoplay.start();
  })
</script>





