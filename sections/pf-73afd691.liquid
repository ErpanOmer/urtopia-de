<style>
  #section1 {
    display: grid;
    grid-template-columns: 1fr 1fr;
    max-width: 1280px;
    align-items: center;
    text-align: center;
    margin: 0 auto;
    padding: 100px 0 60px;
}

#section1 .u17Light {
    line-height: 24px;
    max-width: 600px;
}


#section1 .u17Light > div {
  display: flex;
  margin-top: 24px;
}

#section1 img {
    max-width: var(--sale-collection-image-max-width);
}

@media (max-width: 767px) {
    #section1 {
        padding: 40px 15px;
        flex-direction: column;
    }

    #section1 .u17Light {
        margin-top: 24px;
        font-size: 15px;
        line-height: 21px;
        text-align: center;
    }


    #section1 .u17Light > div {
