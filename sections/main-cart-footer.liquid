    }


  .product_code {
      /* display: flex; */
        margin-bottom: 20px;
  }
  
  .carbon_code {
      max-width: 410px;
      background: url(https://cdn.shopify.com/s/files/1/0633/2068/6808/files/20230324-171240.png?v=1679649232) no-repeat;
