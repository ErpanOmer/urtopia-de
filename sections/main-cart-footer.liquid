<style>
  :root {
    --sale_background_image: linear-gradient(-90deg, var(--sale-color1), var(--sale-color2));
    --sale_background_image-anti: linear-gradient(90deg, var(--sale-color2), var(--sale-color1));
    --sale_text_color: var(--sale-color1);
  }

  .orderTestRideBtn{
    width: 100%;
	height: 50px;
	font: normal normal 600 23px/28px Urbane;
	color: #fff;
	background: #FD4B17;
	border: 2px solid #FD4B17;
	border-radius: 30px;
	margin-bottom: 6px;
	transition:all 0.3s;
	line-height: 50px;
    text-align: center;
	background: #161c1f;
	border: 2px solid #161c1f;
	font-size: 15px;
	cursor: pointer;
    line-height: unset;
      text-align: center;
    background: #fff;
    color: #000;
    border: 2px solid #161c1f;
    display: grid;
      grid-template-columns: 0.5fr 1fr;
      grid-column-gap: 12px;
    align-items: center;
      justify-items: right;
    }

    .orderTestRideBtn:hover {
      opacity: 0.7;
    }
  
