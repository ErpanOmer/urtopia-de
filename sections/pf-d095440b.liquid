    function changeVariant (model, size, style) {
        if (model) {
            $(`.product .product-panel .Model .active`).removeClass('active')
            $(`.product .product-panel .Model > div[attribute="${model}"]`).addClass('active')
            $(`.product .product-panel .options > div[option="Model"] .u17DemiBold_v2 .u17Light_v2`).text(`Urtopia Carbon ${model === 'Carbon Belt' ? '1' : '1s'}`)

            $(`.product .commonComponent .sale .u20DemiBold_v2`).text(`${model === 'Carbon Belt' ? '{{ 269900 | money }}' : '{{ 299900 | money }}'} NOW`)
            $(`.product .commonComponent .sale .u17DemiBold_v2`).text(`with ${model === 'Carbon Belt' ? '{{ settings.sale_name }}' : 'Pre-order Sale'}`)

            $(`.product .count-down .u17Medium_v2`).text(`with ${model === 'Carbon Belt' ? '€600 off + 3 Free Accessories Ends in:' : '€300 off + 3 Free Accessories Ends in:'}`)

            if (model === 'Carbon Belt') {
                //$('.product .belt').show()
                //$('.product .gear').hide()
                

                global_config.event_accessories_variant_ids = ['43817744793816', '43745263255768', '43745261748440'];

