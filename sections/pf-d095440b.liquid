      data-productid="{{ product.id }}"
      id="product_form_{{ product.id }}"
      enctype="multipart/form-data"
    >
      <input
        type="hidden"
        name="id"
        data-product-id="{{product.id}}"
        value="{{ product.selected_or_first_available_variant.id }}"
      >
      <input type="hidden" name="form_type" value="product">
      <input type="hidden" name="utf8" value="✓">
      <div class="commonComponent mobileHide" id="sale_acc">
        <div class="u24DemiBold_v2">{{ product.title }}</div>
        <div class="mb16">
          {% render 'judgeme_widgets',
            widget_type: 'judgeme_preview_badge',
            jm_style: '',
            concierge_install: false,
            product: product
          %}
        </div>

        <div
          class="product-price u30DemiBold_v2"
           data-price="UVP {{ product.compare_at_price | money }}"
        >
          {{ product.price | money }}
        </div>
        <div class="LEASING mb20">
          <span class="u14DemiBold_v2">LEASING</span>
          <span class="u14Medium_v2"><a target="_blank" href="/pages/ebike-leasing">from {{ 4200 |  money_without_trailing_zeros }} monthly</a></span>
        </div>
        <ul class="list mb20 u14Light_v2">
          <li>Price incl. VAT plus {{ 9900 |  money_without_trailing_zeros }} shipping costs</li>
          <li>Delivery to Germany 3-5 working days</li>
        </ul>
        {% render 'sale' %}
      </div>
      <div class="options">
        {% for option in product.options_with_values %}
          {% unless option.name == "Model" %}
            <div class="option mb24" option="{{option.name}}" position="{{ option.position }}">
            <div class="u17DemiBold_v2 mb16">{{ option.name }} - <span class="u17Light_v2"></span></div>
            <div class="{{ option.name }}">
              {% for value in option.values %}
                {% case option.name %}
                  {% when 'Model' %}
                    <div attribute="{{ value }}" class="{{ value }}">
                      {% assign values = value | split: ' · ' %}
                      {% for value in values %}
                        <span>{{ value }}</span>
                      {% endfor %}
                    </div>
                  {% when 'Style' %}
                    <div attribute="{{ value }}" class="{{ value }}">
                      <span class="tooltip">{{ value }}</span>
                    </div>
                  {% else %}
                    <div attribute="{{ value }}" class="{{ value }}">{{ value }}</div>
                {% endcase %}
              {% endfor %}
            </div>
          </div>
          {% endunless %}
        {% endfor %}
      </div>
      <div class="accessories mb12">
        <div class="u17DemiBold_v2 mb12">Accessories</div>
        <div class="prev"></div>
        <div class="next"></div>
        <div class="items mb12">
          {% for prod in collections['on-page'].products %}
            {% if prod.available %}
              {% assign variant = prod.variants | first %}
              <div class="item" variant-id="{{ variant.id }}">
                <div class="u11Medium_v2" style="height: 28px;">{{ prod.title }}</div>
                <div>
                  <div>
                    <span class="u17DemiBold_v2">{{ prod.price | money }}</span>
                    <span class="u11Light_v2">
                      <i class="fa-solid fa-plus"></i>
                    </span>
                  </div>
                  {{ prod.featured_image | image_url: width: 150 | image_tag: loading: 'lazy' }}
                </div>
              </div>
            {% endif %}
          {% endfor %}
        </div>
