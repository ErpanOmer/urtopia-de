<script>
    // 默认隐藏
    $('.bottom-fixed').hide()
    $('.top-fixed').hide()
    $('.toTop').hide()

    const variants = []
    let temp = null

    {% for variant in product.variants %}
      temp = JSON.parse(JSON.stringify({{ variant | json }}))
      temp.inventory_quantity = {{ variant.inventory_quantity }}
      
      variants.push(temp)
    {% endfor %}

    const search = getSearchValues()
    
    const size_map = {
        M: 'Fit for 165-180 cm',
        L: 'Fit for 175-195 cm'
    }

    let current_variant_id = null
    let current_variant = null
    const styles = {}
    const sizes = {}
    const models = {}
    
    const leftSwiper = new Swiper(".product .left-swiper", {
        direction: 'vertical',
        spaceBetween: 8,
        slidesPerView: 8,
        freeMode: true,
        watchSlidesProgress: true,
      })

      const rightSwiper = new Swiper(".product .right-swiper", {
        autoHeight: true,
        navigation: {
          nextEl: ".product .right-swiper .swiper-button-next",
          prevEl: ".product .right-swiper .swiper-button-prev",
        },
        pagination: {el:'.swiper-pagination'},
        effect: 'fade',
        fadeEffect:{
            crossFade: true
        },
        thumbs: {
          swiper: leftSwiper,
        },
      });

    function findVariantOptions (model, size, style) {
        return variants.find(v => `${model} / ${size} / ${style}` === v.title)
    }

    function findVariantFromId (id) {
        return variants.find(v => String(v.id) === String(id))
    }

    function findAvailableVariant () {
        return variants.find(v => v.available)
    }

    function changeVariant (model, size, style) {
        if (model) {
            $(`.product .product-panel .Model .active`).removeClass('active')
            $(`.product .product-panel .Model > div[attribute="${model}"]`).addClass('active')
            $(`.product .product-panel .options > div[option="Model"] .u17DemiBold_v2 .u17Light_v2`).text(`Urtopia Carbon ${model === 'Carbon Belt' ? '1' : '1s'}`)

            //$(`.product .commonComponent .sale .u20DemiBold_v2`).text(`${model === 'Carbon Belt' ? '{{ product.price | minus: settings.carbon_ebike_discount_price | money }}' : '{{ product.price | minus: settings.carbon1s_ebike_discount_price | money }}'} NOW`)
            //$(`.product .commonComponent .sale .u17DemiBold_v2`).text(`with ${model === 'Carbon Belt' ? '{{ settings.sale_name }}' : 'Pre-order Sale'}`)
            //$(`.product .count-down .u17Medium_v2`).text(`with ${model === 'Carbon Belt' ? '{{ settings.carbon_ebike_discount_price | money_without_trailing_zeros }} off + {{ settings.carbon_ebike_free_accessories_count }} Free Accessories Ends in:' : '{{settings.carbon1s_ebike_discount_price | money_without_trailing_zeros}} off + {{ settings.carbon_ebike_free_accessories_count }} Free Accessories Ends in:'}`)

            if (model === 'Carbon Belt') {

                

                global_config.event_accessories_variant_ids = global_config.carbon_order_page_config.events.carbon1_send_accessories_variant_ids

                $(`.product .product-panel .options .option .vanilla`).show();
                $('.buttons .orderTestRideBtn .stages').text('{{ product.price | minus: settings.carbon_ebike_discount_price | divided_by: 24 | money_without_trailing_zeros }}');
                $('.top-fixed .centeral-content > div').html(`
                  <span class="u17Light_v2"><span class="u20DemiBold_v2 emphasize" style="font-weight: 600;color:var(--sale-order-page-text-color);">{{ product.price | minus: settings.carbon_ebike_discount_price | money }}</span> <span style="text-decoration: line-through;">{{ product.price | money }}</span></span>
                  <span class="u14Light_v2">or as low as {{ product.price | minus: settings.carbon_ebike_discount_price | divided_by: 24 | money_without_trailing_zeros }}/Month</span>
              `)

              $('.bottom-fixed > div').html(`
                <span class="u14Light_v2"><span class="emphasize" style="font-weight: 600;color:var(--sale-order-page-text-color);">{{ product.price | minus: settings.carbon_ebike_discount_price | money }}</span> <span style="text-decoration: line-through;">{{ product.price | money }}</span></span>
                <span class="u14Light_v2">or as low as {{ product.price | minus: settings.carbon_ebike_discount_price | divided_by: 24 | money_without_trailing_zeros }}/Month</span>
              `)

              $(`.product .count-down .u17Medium_v2`).text('{{ settings.sale_name }} Ends in:')
              $('.product .commonComponent .product-price').text('{{ product.price | minus: settings.carbon_ebike_discount_price | money }}')
              $('.product .commonComponent .sale-tag .u17Medium_v2').text('{{ settings.sale_name }}')
              $('.product .sale_acc .discount').text('{{ settings.carbon_ebike_discount_price | money_without_trailing_zeros }}')
              $('#announcement-bar .u11Light_v2').text('{{ settings.sale_name }} ends in:')

            } else {
                //$('.product .belt').hide()
                //$('.product .gear').show()
                

                global_config.event_accessories_variant_ids = global_config.carbon_order_page_config.events.carbon1s_send_accessories_variant_ids

                $(`.product .product-panel .options .option .vanilla`).hide();
                $('.buttons .orderTestRideBtn .stages').text('{{ product.price | minus: settings.carbon1s_ebike_discount_price | divided_by: 24 | money_without_trailing_zeros }}');
                $('.top-fixed .centeral-content > div').html(`
                  <span class="u17Light_v2"><span class="u20DemiBold_v2 emphasize" style="font-weight: 600;color:var(--sale-order-page-text-color);">{{ product.price | minus: settings.carbon1s_ebike_discount_price | money }}</span> <span style="text-decoration: line-through;">{{ product.price | money }}</span></span>
                  <span class="u14Light_v2">or as low as {{ product.price | minus: settings.carbon1s_ebike_discount_price | divided_by: 24 | money_without_trailing_zeros }}/Month</span>
              `)

              $('.bottom-fixed > div').html(`
                <span class="u14Light_v2"><span class="emphasize" style="font-weight: 600;color:var(--sale-order-page-text-color);">{{ product.price | minus: settings.carbon1s_ebike_discount_price | money }}</span> <span style="text-decoration: line-through;">{{ product.price | money }}</span></span>
                <span class="u14Light_v2">or as low as {{ product.price | minus: settings.carbon1s_ebike_discount_price | divided_by: 24 | money_without_trailing_zeros }}/Month</span>
              `)

              $(`.product .count-down .u17Medium_v2`).text('Price Going Up in:')
              $('.product .commonComponent .product-price').text('{{ product.price | minus: settings.carbon1s_ebike_discount_price | money }}')
              $('.product .commonComponent .sale-tag .u17Medium_v2').text('Limited Time and Stock')
              $('.product .sale_acc .discount').text('{{ settings.carbon1s_ebike_discount_price | money_without_trailing_zeros }}')
              $('#announcement-bar .u11Light_v2').text('Price Going Up in:')
            }
        }

        if (style) {
            $(`.product .product-panel .Style .active`).removeClass('active')
            $(`.product .product-panel .Style > div[attribute="${style}"]`).addClass('active')
            $(`.product .product-panel .options > div[option="Style"] .u17DemiBold_v2 .u17Light_v2`).text(style)

            let swiper_arr = []

            swiper_arr = swiper_arr.concat(global_config.carbon_order_page_config.product_images[model][style], style === 'vanilla' ? [] : global_config.carbon_order_page_config.product_images[model]['commonSwiper'])

            $('.product .right-swiper .swiper-wrapper').html(swiper_arr.map((s, i) => {
                const index = i % 6

                return  `<div class="swiper-slide">
                            <img src="${s}&width=700"/>
                            <div>
                                <span class="u20DemiBold_v2">${global_config.carbon_order_page_config.product_media_images[index].text}</span>
                                <img src="${global_config.carbon_order_page_config.product_media_images[index].img}&width=350">
                            </div>
                        </div>`
              }).join(''))
            $('.product .left-swiper .swiper-wrapper').html(swiper_arr.map(s => `<div class="swiper-slide"><img src="${s}&width=700"></div>`).join(''))
            
            // update
            setTimeout(() => {
                rightSwiper.update()
                rightSwiper.slideTo(0, 0)
                
                leftSwiper.update()
                leftSwiper.slideTo(0, 0)
            }, 0)
        }

        if (size) {
            $(`.product .product-panel .Size .active`).removeClass('active')
            $(`.product .product-panel .Size > div[attribute="${size}"]`).addClass('active')
            $(`.product .product-panel .options > div[option="Size"] .u17DemiBold_v2 .u17Light_v2`).text(size_map[size])
        }


        if (model && style && size) {
            current_variant = findVariantOptions(model, size, style)
            current_variant_id = current_variant.id

            replaceSearchValue('variant', current_variant_id)
            $('.product .product-panel input[name="id"]').val(current_variant_id)
            $('.product .delivery_time').html(global_config.carbon_order_page_config.ebike_delivery_time[current_variant_id] || global_config.carbon_order_page_config.ebike_default_delivery_time)

            const btn =  $('.product .buttons .addToCartBtn')
            if (current_variant.available) {
                btn.find('span').text('Add to Cart')
                btn.removeClass('disabled')
                $('.product .buttons .orderTestRideBtn').show()
            } else {
                btn.find('span').text('Sold out')
                btn.addClass('disabled')
                $('.product .buttons .orderTestRideBtn').hide()
            }


            if (model === "Shimano Gear" && style === "vanilla") {

              for (const style in styles) {
                if (Object.hasOwnProperty.call(styles, style) && style !== "vanilla") {
                  const f = findVariantOptions(model, size, style)

                  // 查找可用的variant
                  if (f) {
                    changeVariant(f.option1, f.option2, f.option3)
                    break
                  }                  
                }
              }
              
            }

            const getContent = s => {
              const f = findVariantOptions(current_variant.option1, s, current_variant.option3)
    
              if (!f || !f.available) {
                return '{{ 'order_page.out' | t }}'
              }
    
              return `{{ 'order_page.only' | t }} ${Math.max(f.inventory_quantity, 2)} {{ 'order_page.stock' | t }}`
            }
    
            setTimeout(() => {
              $(`.product .product-panel .Size > div[attribute="M"]`).attr('data-inventory-quantity', getContent('M'));
              $(`.product .product-panel .Size > div[attribute="L"]`).attr('data-inventory-quantity', getContent('L'));
            }, 100)
        }

        console.log(current_variant)
    }


    function changeAddToCartText (parse, step = 0) {
        if (step) {
            $(".addToCartBtn i").toggleClass("showi");
            document.querySelector(".addToCartBtn span").innerHTML = "Adding...";
        } else {
            setTimeout(()=>{
                $(".addToCartBtn i").toggleClass("showi");
                document.querySelector(".addToCartBtn span").innerHTML = "Add to Cart";
            }, 800)
        }
    }

    function testRideTrace(type = '', target){

        // 如果已经有按钮 loading ,直接退出来
        if (!type || $('.orderTestRideBtn').hasClass('loading')) {
            return
        }
    
        const t = $(target)
    
        t.addClass('loading')
        // 原本的html 存起来
        const text = t.find('span').html()
        const loading = '<i class="fa fa-spinner fa-spin"></i>Processing...'
    
        // loading
        t.find('span').html(loading)
    
        
    
        if (type === 'lease_on_jobrad') {
            fetchBuried('websiteclick', `carbon-order-page`, { button: type }, true)
    
            t.removeClass('loading')
            t.find('span').html(text)

            const bike_model = current_variant.option1 === 'Carbon Belt' ? 'carbon1' : 'carbon1s'

            const url = window.sessionStorage.localPathPerfix + `/pages/ebike-leasing?bike_model=${bike_model}&bike_size=${current_variant.option2}&bike_style=${current_variant.option3}&utm_source=order_page#forms`

            return window.open(url)
        }
    
        if (type === 'klarna_check_out') {
            fetchBuried('websiteclick', `carbon-order-page`, { button: type, action_type: sessionStorage.carbon_one_a_b_test_flag }, true)
            window.sessionStorage.clicked_klarna_checkout_button = 'clicked_klarna_checkout_button'
    
            setTimeout(() => {
                    t.removeClass('loading')
                    t.find('span').html(text)
                }, 2000)

            // 如果 B test 需要显示 klarna checkout
            window.show_notification_checkout_button = true
            return addToCartInsurance("atc", true)
        }
    
        if (type === 'shop_on_mediamarkt') {
            t.removeClass('loading')
            t.find('span').html(text)
            return window.open('https://www.mediamarkt.de/de/search.html?query=urtopia&filter=carType%3ACitybike')
          
        }
    }

    {% for variant in product.variants %}
        models[{{ variant.option1 | json }}] = 1
        styles[{{ variant.option3 | json }}] = 1
        sizes[{{ variant.option2 | json }}] = 1
    {% endfor %}

    console.log('variants', variants)

    for (const model of Object.keys(models)) {
        $('.product .product-panel .Model').append(`<div data-sale="{{ "order_page.sale" | t }}" attribute="${model}" class="${model}">${model}</div>`)
    }

    for (const style of Object.keys(styles)) {
        $('.product .product-panel .Style').append(`<div attribute="${style}" class="${style}"></div>`)
    }

    for (const size of Object.keys(sizes)) {
        $('.product .product-panel .Size').append(`<div attribute="${size}" class="${size} u14DemiBold_v2">${size === 'M' ? 'Medium' : 'Large' }</div>`)
    }


    $('.product-panel .options .option >div:not(.u17DemiBold_v2) > div').on('click',e => {
        const t = $(e.target)

        if ($(e.target).parent().hasClass('Model')) {
            changeVariant(t.attr('attribute'), current_variant.option2, current_variant.option3)
        } else if ($(e.target).parent().hasClass('Size')) {
            changeVariant(current_variant.option1, t.attr('attribute'), current_variant.option3)
        } else {
            changeVariant(current_variant.option1, current_variant.option2, t.attr('attribute'))
        }
    })

    // 如果有查询参数
    function pageInit() {
      const variant_id = search.variant || global_config.carbon_order_page_config.default_variant
      let find = findVariantFromId(variant_id)
  
      if (!find || !find.available) {
        find = findAvailableVariant()
      }
  
      // 如果默认 variant 也不可用，那晒显出能用的variant
      changeVariant(find.option1, find.option2, find.option3) 
  }



    $('.product .product-panel .accessories .navigate > div').on('click', e => {
        const items = $('.product .product-panel .accessories .items')

        items.animate({
            scrollLeft: $('.product .product-panel .accessories .items').scrollLeft() + ($(e.target).hasClass('next') ? 180 : -180)
        }, 300)
    })

    $('.product .product-panel .accessories .items .item').on('click', e => {
        const item = $(e.target).closest('.item')
        const icon = item.find('.u11Light_v2')

        if (item.hasClass('active')) {
            item.removeClass('active')
            icon.html('<i class="fa-solid fa-plus"></i>')
            
        } else {
            item.addClass('active')
            icon.html('<i class="fa-solid fa-check"></i>')
        }
    })

    $('.product .product-panel').on("submit", function (event) {

        // 阻止默认行为
        event.preventDefault();
        return false;
    })

    $('.product .dialogs > div').on('click', e => {
        const m = {
            0: 'one',
            1: 'two',
            2: 'three',
            3: 'four'
        }

        $('.product .dialogs > div').each((i, t) => {
            if ($(e.target).closest('.u17Medium_v2')[0] === t) {
                console.log(i)

                const layer = document.querySelector(`.order-modal-${m[i]}`);
                const laybox = document.querySelector(`.order-modal-layer-${m[i]}`);
                layer.style.display = "flex";
                laybox.className += " dialog-open";
            }
        })
    })

    $('.toTop').on('click', e => {
        $('html').animate({
            scrollTop: 0
        }, 300)
    })


    let currnetScroolTop = 0
    $(document).on('scroll', throttle(e => {
        const top1 = $('#freedom').offset().top
        const top3 = $('.product .addToCartBtn').offset().top
        const top2 = $(e.target).scrollTop()

        const h = window.screen.height

        if (global_config.is_mobile) {
            if (top2 > top3) {
                $('.bottom-fixed').show()
            } else {
                $('.bottom-fixed').hide()
            }
        } else {
            if (top2 > top1) {
                $('.top-fixed').show()
            } else {
                $('.top-fixed').hide()
            }

            if (currnetScroolTop > top2) {
                $('.top-fixed').hide()
            }

            currnetScroolTop = top2
        }

        top2 > (h * 2) ? $('.toTop').show() : $('.toTop').hide()

    }, 300, 100))

    pageInit()

    jQuery.getJSON(window.Shopify.routes.root + 'products/urtopia-carbon-e-bike.js', res => {
        for (const variant of res.variants) {
          // 如果此配件不可用 
          if (!variant.available) {
            const carbon1_send_accessories_variant_ids = JSON.parse(JSON.stringify(global_config.carbon_order_page_config.events.carbon1_send_accessories_variant_ids))
            const carbon1s_send_accessories_variant_ids = JSON.parse(JSON.stringify(global_config.carbon_order_page_config.events.carbon1s_send_accessories_variant_ids))

            // 查找这个配件 是否在送配件列表里
            const f1 = carbon1_send_accessories_variant_ids.findIndex(i => i === String(variant.id))
            const f2 = carbon1s_send_accessories_variant_ids.findIndex(i => i === String(variant.id))

            // 如果存在, 删除送配件列表 并重写赋值全局列表
            if (f1 >= 0) {
              carbon1_send_accessories_variant_ids.splice(f1, 1)
              global_config.carbon_order_page_config.events.carbon1_send_accessories_variant_ids = carbon1_send_accessories_variant_ids

              changeVariant(current_variant.option1, current_variant.option2, current_variant.option3)
            }

            if (f2 >= 0) {
              carbon1s_send_accessories_variant_ids.splice(f1, 1)
              global_config.carbon_order_page_config.events.carbon1s_send_accessories_variant_ids = carbon1s_send_accessories_variant_ids

              changeVariant(current_variant.option1, current_variant.option2, current_variant.option3)
            }
          }
        }
    })
</script>


