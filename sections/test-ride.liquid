                    ${info.availableSizes ? info.availableSizes.map(i => `<li class="gray">${i}</li>`).join('') : `<li class="gray">Carbon One Size ${info.testRideSize}</li>`}
                </ul>
                <div class="time gray"><img src="https://cdn.shopify.com/s/files/1/0633/2068/6808/files/calendar_2x_af8d9192-1ff9-43f0-aba6-3547b1129854.jpg?v=1680938382"/> Earliest available time: <div>&nbsp;${info.avalibaleDate}</div></div>
            `)
            
            // 点击item 代理到真实的store
            item.on('click', e => {
                const active = $('#storepoint-panel .items .item[active="true"]')
                const locations = $('#storepoint-results-container .storepoint-location')

                

                if (active.length) {
                    active.attr('active', false)
                }


                locations.each((i, elemm) => {
                    const iterator = $(elemm)
                    iterator.removeClass('selected')
                    if (item.attr('name').replace(/\s*/g, "") === iterator.find('.storepoint-name').text().replace(/\s*/g, "")) {
                        iterator.addClass('selected')
                        iterator[0].click()

                        return false
                    }
                })

                item.attr('active', true)

                document.body.clientWidth < 768 && $('html').scrollTop($('.map').offset().top)

                last_select_store = item.attr('name')
                select_store_count += 1
            })

            items.append(item)

            

            if (!options.find(option => option.attr('city') === info.testrideSpot )) {
                options.push(option)
            }

            // 异步计算
            setTimeout(calculateFunc, 300, info, item)
        })

        // 首字母排序
        options.sort((a, b) => a.attr('city').localeCompare(b.attr('city')))

        // select group
        const optgroup = []
        for (const option of options) {
            const country = option.attr('country')
            // find
            let group = optgroup.find(o => o.attr('label') === country)

            if (!group) {
                group = $(document.createElement('optgroup'))
                group.attr('label', country)
                optgroup.push(group)
            }

            group.append(option)
        }

        for (const group of optgroup) {
            select.append(group)
        }

        for (const bike of bike_options) {
            bike_select.append(bike)
        }

        storeList.append(items)
        storeList.append(no_result)
        forms.append(form)

        $(target).parent().append(storeList)
        $('#storepoint-search').append(forms)

        fetchBuried('testride', 'performance', {
            store_list_render_total_time: `${+new Date() - store_list_render_total_time} ms`,
            store_list_calculate_total_time: `${store_list_calculate_total_time} ms`,
            disable_date_max_limit,
            disable_date_max_interval_limit,
            disable_date_max_loop_limit,
            userAgent: navigator.userAgent,
            userAgentData: navigator.userAgentData,
        })
    }

    function observerItems(mutationsList) {
        const no_result = document.querySelector('#storepoint-panel .no_result')
        no_result.style.display = 'none'

        const results = $('#storepoint-results-container .storepoint-location')
        

        $('#storepoint-panel .items .item').each((_, e) => {
            const iterator = $(e)
            iterator.hide()

            results.each((__, ee) => {
                const result = $(ee)

                const name1 = result.find('.storepoint-name').text() || ''
                const name2 = iterator.attr('name') || ''

                if (name1.replace(/\s*/g, "") === name2.replace(/\s*/g, "")) {
                    iterator.show()
                    return false
                }
            })
        })

        if (results.length === 0 && document.querySelector('#storepoint-results-container .storepoint-no-results')) {
            no_result.style.display = 'flex'
